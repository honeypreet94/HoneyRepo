<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
		 
		 
<beans>
	<bean id="t" class="main.SecondaryCar">
	
	<property name="engine">
		<list>
			<ref bean="e1"></ref>
			<ref bean="e2"></ref>
		</list>

	</property>
	
	</bean>
	
	<bean id="e1" class="main.SecondaryEngine">
			<property name="modelYear" value="2005"/>
		</bean>
		
<bean id="e2" class="main.SecondaryEngine">
			<property name="modelYear" value="1994"/>
		</bean>
	
</beans>